<h1><%= "Welcome #{current_user.username}!" %></h1>

<%= button_to "Discover Movies", discover_path, method: :get %> <br>

<h3>Friends</h3> <br>

<%= form_with url: dashboard_path, local: true do |f| %>
  <%= f.label :email, "Friend's Email" %>
  <%= f.text_field :email %>
  <%= f.submit "Add Friend" %>
<% end %>
