<%= form_with model: @party, local: true do |f|  %>

  <%= f.label :movie_title %>
  <%= f.text_field :movie_title, value: @movie_title ,:readonly => true %>

  <%= f.label :party_duration %>
  <%= f.number_field :party_duration, value: @movie_runtime %>

  <%= f.label :party_date %>
  <%= f.date_field :party_date %>

  <%= f.submit 'Create Party' %>
<% end %>
