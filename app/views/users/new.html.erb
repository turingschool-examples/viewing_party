<h1>Register</h1>

<%= form_with model: @user, local: true do |f| %>

  <%= f.label :name %>
  <%= f.text_field :name %>

  <%= f.label :email %>
  <%= f.text_field :email %>

  <%= f.label :password %>
  <%= f.password_field :password, value: nil %>

  <%= f.label :password_confirmation %>
  <%= f.password_field :password_confirmation, value: nil %>

  <%= f.submit 'Register' %>
<% end %>
