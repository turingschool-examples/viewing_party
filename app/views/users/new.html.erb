<h1>Please Register an Account</h1>

<%= form_tag "/registration", method: :post do %>
  <%= label_tag :email %>
  <%= text_field_tag :email %>

  <%= label_tag :password %>
  <%= password_field_tag :password %>

  <%= label_tag :password_confirmation %>
  <%= password_field_tag :password_confirmation %>

  <%= submit_tag "Register" %>
<% end %>
