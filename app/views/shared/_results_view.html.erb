<div class='movies'>
    <% @movies_info.each do |movie| %>
    <section class='movie' id='movie=<%= movie.movie_db_id %>'>
        <ul>
            <li><%= link_to movie.title, "/movies/#{movie.movie_db_id}" %></li>
            <li>Vote Average: <%= movie.vote_average %></li>
        </ul>
    </section>
    <% end %>
</div>
