<%= form_with url: login_path, local: true do |form| %>
  <%= form.label :email, 'Email:' %>
  <%= form.email_field :email %>
  
  <%= form.label :name, 'Name:' %>
  <%= form.text_field :name %>

  <%= form.label :password, 'Password:' %>
  <%= form.password_field :password %>

  <%= form.submit 'Log In' %>
<% end %>
